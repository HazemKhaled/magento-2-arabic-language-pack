{
  "name": "mp",
  "version": "1.11.0",
  "private": true,
  "description": "Knawat MarketPlace API layer",
  "author": "Knawat, LLC.",
  "engines": {
    "node": "12.x.x"
  },
  "scripts": {
    "build": "npm run clean && npm run tsc && npm run webpack:prod",
    "clean": "rm -rf build",
    "tsc": "tsc",
    "webpack:prod": "webpack --env platform=app --env production --config webpack.prod.js",
    "webpack:dev": "webpack --config webpack.dev.js",
    "dev": "npm run lint && ts-node ./node_modules/moleculer/bin/moleculer-runner.js --envfile ./docker/development/.env --hot --repl --config moleculer.config.ts services/**/*.service.ts",
    "dev:frontend": "npm run dev & npm run webpack:dev",
    "cli": "moleculer connect redis://redis",
    "start": "moleculer-runner --config build/moleculer.config.js",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "npm run test:unit && npm run test:integration ",
    "pretest:integration": "jest getstore.spec.ts",
    "test:integration": "jest integration.spec.ts -- runInBand",
    "posttest:integration": "rm -rf test/payload/*.json",
    "test:unit": "jest unit.spec.ts --coverage",
    "lint": "eslint . --ext .ts,.tsx --quiet",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "openapi": "node ./utilities/scripts/docs-checker.script.js",
    "prettier": "prettier services/*.ts utilities/**/*.ts *.ts --write"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@shopify/eslint-plugin": "^39.0.3",
    "@types/bluebird": "^3.5.32",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.19.6",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^3.4.9",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-hot-middleware": "^2.25.3",
    "babel-loader": "^8.2.1",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "mini-css-extract-plugin": "^0.11.3",
    "moleculer-repl": "^0.6.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.2",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "@elastic/app-search-node": "^7.9.0",
    "@google-cloud/pubsub": "^2.6.0",
    "@google-cloud/tasks": "^2.1.2",
    "@jest/types": "^26.6.2",
    "@moleculer/lab": "^0.6.0",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "elastic-apm-node": "^3.8.0",
    "elasticsearch": "^16.7.1",
    "form-data": "^2.5.1",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moleculer": "^0.14.11",
    "moleculer-cron": "0.0.2",
    "moleculer-db": "^0.8.11",
    "moleculer-db-adapter-mongo": "^0.4.10",
    "moleculer-elasticsearch": "^1.1.5",
    "moleculer-web": "^0.9.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.16",
    "openapi-types": "^7.0.1",
    "payment": "^2.4.0",
    "qs": "^6.9.4",
    "uuid": "^3.4.0",
    "vue": "^2.6.12",
    "vue-i18n": "^8.21.1",
    "vue-router": "^3.4.5"
  }
}