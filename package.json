{
  "name": "mp",
  "version": "1.9.0",
  "private": true,
  "description": "Knawat MarketPlace API layer",
  "author": "Knawat, LLC.",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "clean": "rm -rf build",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "webpack": "webpack --config webpack.prod.js",
    "webpack:watch": "webpack --watch --config webpack.dev.js",
    "build": "npm run clean && npm run tsc & npm run webpack",
    "build:watch": "npm run tsc:watch & npm run webpack:watch",
    "dev": "env $(cat ./docker/development/.env) ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts services/**/*.service.ts",
    "cli": "moleculer connect TCP",
    "start": "env $(cat ./docker/development/.env) ./node_modules/moleculer/bin/moleculer-runner.js services",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "openapi": "node ./utilities/scripts/docs-checker.script.js",
    "prettier": "prettier services/*.ts utilities/**/*.ts *.ts --write"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "^39.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/compression": "^1.7.0",
    "@types/jest": "^24.9.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.12.62",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^3.4.9",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@typescript-eslint/parser": "^4.3.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.8.0",
    "mini-css-extract-plugin": "^0.11.2",
    "moleculer-repl": "^0.6.4",
    "moleculer-sentry": "0.0.9",
    "prettier": "^2.1.2",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^26.4.0",
    "ts-loader": "^8.0.4",
    "ts-node": "^8.10.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.7",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "@elastic/app-search-node": "^7.9.0",
    "@google-cloud/pubsub": "^2.5.0",
    "@google-cloud/tasks": "^2.1.1",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "elasticsearch": "^16.7.1",
    "form-data": "^2.5.1",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mkdirp": "^0.5.1",
    "moleculer": "^0.14.11",
    "moleculer-cron": "0.0.2",
    "moleculer-db": "^0.8.10",
    "moleculer-db-adapter-mongo": "^0.4.10",
    "moleculer-elasticsearch": "^1.1.5",
    "moleculer-web": "^0.8.5",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.11",
    "uuid": "^3.4.0",
    "vue": "^2.6.12"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  }
}
