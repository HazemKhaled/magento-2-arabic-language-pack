{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "curly": false,
    "completed-docs": [
      true,
      "classes",
      "enums",
      "enum-members",
      "functions",
      "interfaces",
      "namespaces",
      "types"
    ]
  }
}
